
@{
    ViewBag.Title = "ForgetPassword";
    Layout = "~/Areas/Admin/Views/Shared/_beforeLoginAdminLayout.cshtml";
}

<div id="wrapper" class="wcmp-wrapper">
    <div class="login_form forgot-pass">
        <div class="form_row ">
            <form action="#" method="post">
                <h2>Forgot Password</h2>
                <p>You can reset your password here.</p>
                <div class="form-group">
                    <!-- <label><i class="fas fa-user"></i>Enter your Email address</label> -->
                    <input class="form-control" name="username" placeholder="Enter your Email address" type="text">

                </div>
                <div class="form-group btn_sign">
                    <a href="javascript:void(0)" class="btn_submit">Reset Password<a/>
                </div>
            </form>
        </div>
    </div>
</div>

