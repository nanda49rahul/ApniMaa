                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" 
                            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>

                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav mr-auto">
                            <li class="nav-item active">
                                <a class="nav-link" href="/dashboard/index"><span class="icon dash-icon"></span>Dashboard </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/dashboard/prospects"><span class="icon prospect-icon"></span>Prospects </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/dashboard/myteam"><span class="icon team-icon"></span>My Team </a>
                            </li>


                        </ul>

                    </div>

                    <span class="noti">?</span>

                    <div class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <img src="~/content/images/user.png">jhone williams
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" href="/dashboard/account">Account</a>
