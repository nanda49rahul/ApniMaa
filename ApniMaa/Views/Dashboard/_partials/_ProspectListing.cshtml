@model PagingResult<ProspectListingModel>


@if (Model.List != null && Model.List.Count() > 0)
{

    foreach (var item in Model.List)
    {
        <tr class="odd gradeX">
            <td>@item.firstName @item.lastName</td>
            <td>Healthcare Professionals</td>
            <td>@item.status</td>
            <td>@item.addedDate</td>
            <td>@item.modifiedDate</td>
             
            <td class="center">
                <a class="" href="" data-original-title="Edit" 
                   data-toggle="tooltip"><img src="~/content/images/edit-table.png"></a>

                <a data-employeeid="@item.id" class="DeleteEmployee" href="javascript:void(0);" 
                   data-original-title="Remove" data-toggle="tooltip"><img src="~/content/images/cross-table.png"></a>
            </td>
        </tr>
    }
}
else
{
    <tr class="odd gradeX">
        <td colspan="4">No Record Found</td>
    </tr>
}


